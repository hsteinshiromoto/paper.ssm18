  pages = {1019--1027},
  file = {ScienceDirect Full Text PDF:/Users/ian/Library/Application Support/Firefox/Profiles/d1lqfos9.default/zotero/storage/KE4H3IKM/RÃ¼ffer et al. - 2010 - Connection between cooperative positive systems an.pdf:application/pdf;ScienceDirect Snapshot:/Users/ian/Library/Application Support/Firefox/Profiles/d1lqfos9.default/zotero/storage/GNF9AASU/S0005109810001329.html:text/html}
